{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**", "public/dist/**"],
      "inputs": ["$TURBO_DEFAULT$", ".env.production"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "cache": false
    },
    "eslint:fix": {
      "cache": false
    },
    "db:push": {
      "cache": false,
      "interactive": true,
      "persistent": true
    },
    "prod:db:push": {
      "cache": false,
      "interactive": true,
      "persistent": true
    }
  }
}
